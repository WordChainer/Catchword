extends layout

block Content
    main
        .container-fluid
            .row.no-gutters.mt-3
                each text, i in ['전체 단어', '숨겨진 단어', '3-3 단어', '3-2 단어']
                    .col-sm-3.col-6
                        .counter-box
                            span.counter= counts[i]
                            p= text

block JS
    script(src="../js/index.min.js")
